
.list{
	width: 100%;
	height: 100rpx;
	border-bottom: 1rpx solid #e5e5e5;
	background-color: #fff;
	padding: 0 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	line-height: 100rpx;
	font-size: 30rpx;
	color: #333;
}
.list .left{
	width: 200rpx;
	height: 100rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	overflow: hidden;
}
.list .right{
	width: 400rpx;
	height: 100rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	overflow: hidden;
	text-align: right;
}
.list .text{
	margin-right: 10rpx;
}
.header{
	height: 160rpx;
	line-height: 160rpx;
	margin-top: 20rpx;
}
.header .fr{
	height: 160rpx;
}
.header .img_box{
	display: inline-block;
	width: 100rpx;
	height: 100rpx;
	vertical-align: middle;
	border-radius: 50%;
}
.header .img_box image{
	width: 100%;
	height: 100%;
	border-radius: 50%;
}
.header ._i{
	display: inline-block;
	vertical-align: middle;
	margin-left: 10rpx;
}
.space{
	margin-top: 20rpx;
}


