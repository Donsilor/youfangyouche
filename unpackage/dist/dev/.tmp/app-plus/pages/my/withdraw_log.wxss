
.box{
	padding: 0 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.box .list{
	padding: 30rpx 0;
	border-bottom: 1rpx solid #ddd;
}
.box .list .type{
	font-size: 30rpx;
	color: #535353;
}
.box .list .type .right{
	color: #04b521;
}
.box .list .type .red{
	color: #fe3434;
}
.box .list .num{
	font-size: 26rpx;
	color: #888;
	margin: 16rpx 0;
}
.box .list .num .bank ._span{
	margin-right: 10rpx;
}
.box .list .num .date ._span{
	margin-left: 20rpx;
}

