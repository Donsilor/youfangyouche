
.banner{
	width: 750rpx;
	height: 370rpx;
}
.banner image{
	width: 100%;
	height: 100%;
}
.apply{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	width: 750rpx;
	height: 110rpx;
	font-size: 30rpx;
	color: #050505;
	border-bottom: 20rpx solid #f5f5f5;
}
.apply .list{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.apply .img{
	width: 74rpx;
	height: 74rpx;
	margin-right: 20rpx;
}
.apply .my .img{
	background: url('https://szdbi.oss-cn-shenzhen.aliyuncs.com/youfangyouche/house_01.png') no-repeat center;
	background-size: 100% 100%;
}
.apply .now .img{
	background: url('https://szdbi.oss-cn-shenzhen.aliyuncs.com/youfangyouche/house_02.png') no-repeat center;
	background-size: 100% 100%;
}
.title{
	text-align: center;
}
.title .icon{
	display: inline-block;
	width: 34rpx;
	height: 18rpx;
	background: url('https://szdbi.oss-cn-shenzhen.aliyuncs.com/youfangyouche/house_03.png') no-repeat center;
	background-size: 100% 100%;
}
.title .inline{
	display: inline-block;
	font-size: 34rpx;
	color: #333;
	font-weight: bold;
	margin: 30rpx 10rpx;
}
.box{
	padding-bottom: 120rpx;
	width: 710rpx;
	margin-left: 20rpx;
}
.box .list{
	margin-bottom: 30rpx;
}
.box .list .top{
	font-size: 40rpx;
	color: #333;
	margin-bottom: 10rpx;
}
.box .list .top .line{
	width: 8rpx;
	height: 30rpx;
	background-color: #fc921f;
	margin: 12rpx 20rpx 0 0;
}
.box .list .text{
	font-size: 30rpx;
	color: #666;
}


