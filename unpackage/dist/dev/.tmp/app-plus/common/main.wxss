














/*每个页面公共css */
page{
	max-width: 750px;
	margin: 0 auto !important;
}
/* reset */
page,page,._div,._span,._iframe,._h1,._h2,._h3,._h4,._h5,._h6,._em,._img,._strong,._sub,._sup,._b,._i,._dl,._dt,._dd,._ol,._ul,._li,form,label,._table,._tbody,._tfoot,._thead,._tr,._th,._td,._p,canvas,._footer,._header,._nav,audio,video {
	margin:0;
	padding:0;
}
._h1,._h2,._h3,._h4,._h5,._h6,._em,._i {
	font-weight:100;
	font-style:normal;
}
._ul,._ol,._li {
	list-style-type:none;
}
input {
	outline: none;
	border: none;
}
button:after{
	border: 0;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.clf:after{
	display: block;
	content: '.';
	visibility: hidden;
	opacity: 0;
	height: 0;
	clear: both;
}
.btn_r{
	width: 650rpx;
	height: 94rpx;
	background: -webkit-gradient(linear, left top, right top, from(#ff8e06), to(#ff5011));
	background: -o-linear-gradient(left, #ff8e06, #ff5011);
	background: linear-gradient(left, #ff8e06, #ff5011);
	border-radius: 47rpx;
	font-size: 28rpx;
	color: #fff;
	text-align: center;
	line-height: 94rpx;
	margin: 90rpx auto 0;
	opacity: .5;
}
.btn_r.on{
	opacity: 1;
}
.bgcolor_r{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background-color: #f5f5f5;
	z-index: -2;
}
